@model IEnumerable<MVC_Thue_Oto.Models.LoaiChoNgoi>

<div class="backroud_right_1">
    <div class="color_top1_r">Loại chổ ngồi</div>
</div>
@foreach (var item in Model) {
   <div>
       <a href="@Url.Action("TheoLoaiChoNgoi","IndexThueXe",new{id = item.MaChoNgoi})" class="list-group-item list assad_1">
